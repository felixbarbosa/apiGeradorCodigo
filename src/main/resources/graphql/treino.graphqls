type Treino {
        id: Int,
        nome: String,
        exercicio: Exercicio,
        aluno: Aluno,
        repeticoes: String,
        velocidade: String,
        descanso: String
        musculoAlvo: Musculo,
        diaSemana: DiaSemana
        series: String,
        instrucao: String,
        variacaoExercicio: VariacoesExercicios 
}
input TreinoInput {
        id: Int!,
        nome: String!,
        exercicio: Int!,
        aluno: Int!,
        repeticoes: String!,
        velocidade: String!,
        descanso: String!
        musculoAlvo: Int!,
        diaSemana: Int!
        series: String!,
        instrucao: String!,
        variacaoExercicio: Int
}
extend type Query {
        obterTreinoAluno(alunoId: Int!):[Treino]!
        obterTreinoAlunoPorDia(alunoId: Int!, diaSemanaId: Int!):[Treino]!
        obterTreinoAlunoPorMusculo(alunoId: Int!, musculoId: Int!):[Treino]!
        obterTreinoAlunoNome(alunoId: Int!, nome: String!):[Treino]!
}

extend type Mutation{
        salvarTreino(Treino:TreinoInput!): Treino!    
}