type Exercicio {
        id: Int!,
        descricao: String!,
        musculo: Musculo!,
        professor: Personal!
}
input ExercicioInput {
        id: Int!,
        descricao: String!,
        musculo: Int!,
        professor: Int!
}
extend type Query {
        obterExercicioPorId(id:Int!):Exercicio!
        obterExerciciosPorMusculo(musculoId:Int!):[Exercicio]!
        obterExercicios:[Exercicio]!
}

extend type Mutation{
        salvarExercicio(exercicio:ExercicioInput!): Exercicio!
        deletarExercicio(id: Int!): Boolean 
}