type Pessoa {
        id: Int!,
        nome: String!,
        email: String!,
        cref: String,
        cpf: String,
        aluno: Aluno,
        personal: Personal
}
input PessoaInput {
        id: Int!,
        nome: String!,
        email: String!,
        cref: String,
        cpf: String,
        aluno: Int,
        personal: Int
}
extend type Query {
        obterPessoa(id: Int!):Pessoa!
}

extend type Mutation{
        salvarPessoa(pessoa:PessoaInput!): Pessoa!    
}